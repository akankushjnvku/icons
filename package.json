{
  "name": "@transferwise/icons",
  "version": "0.3.1",
  "description": "TransferWise SVG icons",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && npm run demo:build",
    "build": "npm-run-all build:clean build:sprite build:styles",
    "build:clean": "rimraf dist",
    "build:sprite": "node createSprite",
    "build:styles": "cleancss -o dist/icons.min.css styles.css",
    "demo:build": "node buildDemo",
    "demo:deploy": "gh-pages -d demo",
    "test": "npm-run-all check-release-to-github lint",
    "check-release-to-github": "release-to-github-with-changelog-pre-release-checks",
    "lint": "eslint index.js src",
    "release-to-github": "release-to-github-with-changelog"
  },
  "repository": {
    "type": "git",
    "fullname": "transferwise/icons",
    "url": "git+https://github.com/transferwise/icons.git"
  },
  "author": "TransferWise Ltd.",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/transferwise/icons/issues"
  },
  "homepage": "https://github.com/transferwise/icons#readme",
  "dependencies": {
    "clean-css-cli": "^4.1.10",
    "create-svg-icon-sprite": "^0.1.0",
    "gh-pages": "^1.0.0",
    "npm-run-all": "^4.1.1",
    "release-to-github-with-changelog": "^1.1.5"
  },
  "devDependencies": {
    "eslint": "^4.10.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0"
  }
}
